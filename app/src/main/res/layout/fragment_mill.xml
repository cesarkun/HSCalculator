        android:layout_marginTop="8dp"
        card:layout_constraintVertical_bias="0.0" />

    <TextView
        android:text="-"
        android:id="@+id/tv_mill_cards_minus"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        card:layout_constraintLeft_toLeftOf="@+id/gl_mill_vertical20"
        card:layout_constraintTop_toTopOf="@+id/gl_mill_horizontal16"
        card:layout_constraintBottom_toTopOf="@+id/gl_mill_horizontal33"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:minHeight="25dp"
        android:minWidth="25dp"
        card:layout_constraintRight_toLeftOf="@+id/gl_mill_vertical33"
        android:layout_marginEnd="8dp"
        card:layout_constraintVertical_bias="0.0" />

    <TextView
        android:text="+"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_mill_cards_plus"
        android:minHeight="25dp"
        android:minWidth="25dp"
        android:gravity="center"
        card:layout_constraintLeft_toLeftOf="@+id/gl_mill_vertical20"
        card:layout_constraintRight_toLeftOf="@+id/gl_mill_vertical33"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="8dp"
        card:layout_constraintTop_toTopOf="parent"
        card:layout_constraintBottom_toTopOf="@+id/gl_mill_horizontal16"
        card:layout_constraintVertical_bias="1.0"
        android:layout_marginBottom="8dp"
