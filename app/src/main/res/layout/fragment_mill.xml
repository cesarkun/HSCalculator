        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:gravity="center"
        android:minHeight="25dp"
        android:minWidth="25dp"
        card:layout_constraintLeft_toLeftOf="@+id/gl_mill_vertical20"
        card:layout_constraintRight_toLeftOf="@+id/gl_mill_vertical33"
        android:layout_marginEnd="8dp"
        card:layout_constraintTop_toTopOf="@+id/gl_mill_horizontal50"
        card:layout_constraintBottom_toTopOf="@+id/gl_mill_horizontal66"
